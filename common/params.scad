// Value of kerf to apply when lasercutting things.
// Value is based on the experience using Epilog Mini machine in
// Google WFC Makerspace.
kerf = 0.2;

// Spacing between insert and miniatures bases to allow to slick fit.
insert_spacing = 0.2;
magnet_spacing = 0.15;

// Default values
//
// Default diameter of magnets used for my miniatures.
default_magnet_diameter = 3;
base_height = 3;
pin_height = 2.5;